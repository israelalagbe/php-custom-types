{
    "name": "israelalagbe/php-custom-types",
    "description": "This package holds some custom PHP types such as Array and String. It provides functionalities similar to array and string in Javascript.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Israel Alagbe",
            "email": "israelalagbe53@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "IsraelAlagbe\\CustomTypes\\": "src/IsraelAlagbe/CustomTypes/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "platform-check": false
    }
}
