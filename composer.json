{
    "name": "israelalagbe/php-custom-types",
    "description": "This package holds some custom PHP types such as Array and String. It provides functionalities similar to array and string in Javascript.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Israel Alagbe",
            "email": "israelalagbe53@gmail.com"
        },
        {
            "name": "Adedoyin Akande",
            "email": "aeadedoyin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "CustomTypes\\": "/src/CustomTypes/",
            "Tests\\": "/Tests/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit "
    }
}